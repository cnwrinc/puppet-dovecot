# File managed by puppet
## MAILBOX CONFIG
<% if scope.lookupvar('dovecot::params::dovecot_mail_location') -%>
mail_location = <%= scope.lookupvar('dovecot::params::dovecot_mail_location') %>
<% end -%>
<% if scope.lookupvar('dovecot::params::dovecot_mail_gid') -%>
mail_gid = <%= scope.lookupvar('dovecot::params::dovecot_mail_gid') %>
<% end -%>
<% if scope.lookupvar('dovecot::params::dovecot_mail_uid') -%>
mail_uid = <%= scope.lookupvar('dovecot::params::dovecot_mail_uid') %>
<% end -%>
<% if scope.lookupvar('dovecot::params::dovecot_mail_privileged_group') -%>
mail_privileged_group = <%= scope.lookupvar('dovecot::params::dovecot_mail_privileged_group') %>
<% else -%>
mail_privileged_group = mail
<% end -%>
